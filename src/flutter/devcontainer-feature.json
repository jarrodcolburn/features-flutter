{
    "name": "Flutter",
    "id": "flutter",
    "version": "0.0.1",
    "description": "Install the Flutter SDK",
    "containerEnv": {
        "FLUTTER_ROOT": "/usr/local/flutter",
        "PATH": "/usr/local/flutter/bin:${PATH}"
    },
    "options" : {
        "channel" : {
            "type" : "string",
            "enum" : ["stable","dev","beta"],
            "default" : "stable",
            "description" : "Flutter channel"
        }
    }
}